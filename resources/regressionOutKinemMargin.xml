<?xml version="1.0" encoding="UTF-8"?>
 
  <GaussianMixture>
    <!--
    # input - number of features
    # output - number of outputs
    # The number of elements in polynomial - 'm'
    # the maximal number of iterations
    # the number of learning data
    # the number of test data
    # the final equation form
      # 1 - octave/matlab like type
      # 2 - c/c++ like type
    -->
<!--     <method>CMAES</method> -->
    <method>PSO</method>
	<CMAESconfig>optimizeGMmargin.xml</CMAESconfig>
    <parameters inputsNo="3" outputsNo="1" populationSize="40" gaussiansNo="200" maxIterations="5" trainSize="10000" testSize="10000" outputType="2" normalizeOutput="true"></parameters>
    
  </GaussianMixture>
  <trainingSet>kinemOutMarginTrainXYZ5.dat</trainingSet>
  <testSet>kinemOutMarginTrainXYZ5.dat</testSet>
  <verificationSet>kinemOutMarginTrainXYZ5.dat</verificationSet>
